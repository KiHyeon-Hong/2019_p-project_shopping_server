<!doctype html>
<html class="no-js" lang="zxx">

<% include inc-head %>

<body>
	<!-- Main wrapper -->
	<div class="wrapper" id="wrapper">

		<% include inc-header %>

		<!-- Start Bradcaump area -->
		<div class="ht__bradcaump__area bg-image--7">
			<div class="container">
				<div class="row">
					<div class="col-lg-12">
						<div class="bradcaump__inner text-center">
							<h2 class="bradcaump-title"><i class="fas fa-tag fa-2x" aria-hidden="true"></i></h2>
							<nav class="bradcaump-content">
								<span class="brd-separetor">쇼핑몰 명 변경</span>
							</nav>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- End Bradcaump area -->
		<!-- Start main Content -->
		<div class="maincontent bg--white pt--80 pb--55">
			<div class="container chswhite mb-5">
				<div class="row">

					<div class="col-lg-12 col-12">
						<h3>쇼핑몰명 변경</h3>

						<form name="editform" method="post" >
							<div class="wn__single__product">
								<div class="row m-5" style=" justify-content: center; ">
									<div class="col-6">

										<!-- <label for="hello"
												style=" padding: 10px; "
												>쇼핑몰 명</label> -->
										<div class="row">
											<div class="col-9">
												<input class="form-control" id="hello" name="name" type="text" maxlength="15"
													value="<%=name%>" placeholder="쇼핑몰 명">

											</div>
											<div class="col-3">
												<button type="button" onclick="javascript:document.editform.submit();"
													style=" background-color: #e59285; border-color: #e59285; background: #e59285 none repeat scroll 0 0; border: 0 none; color: #fff; font-size: 14px; font-weight: 600; height: 40px; line-height: 40px; padding: 0 25px; text-transform: uppercase; ">
													적용</button>

											</div>
										</div>
									</div>

								</div>

							</div>

						</form>
					</div>
					<div class="col-lg-3 col-12 md-mt-40 sm-mt-40">

					</div>
				</div>
			</div>
			<!-- End main Content -->



			<% include inc-footer %>



		</div>
		<!-- //Main wrapper -->



		<% include inc-script %>


</body>

</html>